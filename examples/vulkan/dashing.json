{
    "name": "Vulkan",
    "package": "vulkan",
    "icon32x32": "icon@2x.png",
    "ExternalURL": "https://www.khronos.org/registry/vulkan/specs/1.0-wsi_extensions/xhtml/vkspec.html",
    "index": "index.html",
    "allowJS": false,
    "selectors": {
        "p > a[id^=\"vk\"]": {
          "type": "Command",
          "attr": "id"
        },
        "p + pre.programlisting": [
            {
                "requiretext": "^typedef struct",
                "type": "Struct",
                "regexp": "^typedef struct ([[:word:]]+) [[:ascii:]]+$",
                "replacement": "${1}"
            },
            {
                "requiretext": "^typedef enum",
                "type": "Enum",
                "regexp": "^typedef enum ([[:word:]]+) [[:ascii:]]+$",
                "replacement": "${1}"
            },
            {
                "requiretext": "^VK_DEFINE_",
                "type": "Object",
                "regexp": "^VK_DEFINE_[_A-Z]*HANDLE\\(([[:word:]]+)\\)$",
                "replacement": "${1}"
            }
        ],
        "h2.title": {
            "requiretext": "^C\\.[0-9]\\. VK_KHR_",
            "type": "Extension",
            "regexp": "^C\\.[0-9]\\. VK_KHR_([[:word:]]+)$",
            "replacement": "VK_KHR_${1}"
        },
        "h3.title": {
            "requiretext": "^C\\.[0-9]\\.[0-9]+\\. VK_KHR_",
            "type": "Extension",
            "regexp": "^C\\.[0-9]\\.[0-9]+\\. VK_KHR_([[:word:]]+)$",
            "replacement": "VK_KHR_${1}"
        },
        "div.section > div.itemizedlist > ul.itemizedlist > li.listitem > em.parameter:first-child > code": "Parameter",
        "dt > span.term > a.link[title=\"Error codes\"]": "Error",
        "h1": "Section",
        "h2": "Section"
    },
    "ignore": [
      ""
    ]
}
