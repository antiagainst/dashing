{
    "name": "Vulkan",
    "package": "vulkan",
    "icon32x32": "icon@2x.png",
    "ExternalURL": "https://www.khronos.org/registry/vulkan/specs/1.0-wsi_extensions/xhtml/vkspec.html",
    "index": "vkspec.html",
    "allowJS": false,
    "selectors": {
        "div.listingblock[id^=\"vk\"] > div.content > pre.CodeRay.highlight > code[data-lang=\"c++\"]": {
          "requiretext": "^[[:word:]]+ [[:word:]]+ ?\\(",
          "type": "Command",
          "regexp": "^[[:word:]]+ ([[:word:]]+) ?\\([[:ascii:]]+$",
          "replacement": "${1}"
        },
        "div.listingblock[id^=\"Vk\"] > div.content > pre.CodeRay.highlight > code[data-lang=\"c++\"]": [
            {
                "requiretext": "^typedef struct",
                "type": "Struct",
                "regexp": "^typedef struct ([[:word:]]+) [[:ascii:]]+$",
                "replacement": "${1}"
            },
            {
                "requiretext": "^typedef union",
                "type": "Union",
                "regexp": "^typedef union ([[:word:]]+) [[:ascii:]]+$",
                "replacement": "${1}"
            },
            {
                "requiretext": "^VK_DEFINE_[_A-Z]*HANDLE",
                "type": "Object",
                "regexp": "^VK_DEFINE_[_A-Z]*HANDLE\\(([[:word:]]+)\\)$",
                "replacement": "${1}"
            }
        ],
        "div.listingblock > div.content > pre": {
            "requiretext": "^typedef enum",
            "type": "Enum",
            "regexp": "^typedef enum ([[:word:]]+) [[:ascii:]]+$",
            "replacement": "${1}"
        },
        "h3[id^=\"VK_KHR_\"]": {
            "requiretext": "^VK_KHR_",
            "type": "Extension",
            "regexp": "^VK_KHR_([[:word:]]+)$",
            "replacement": "VK_KHR_${1}"
        },
        "div.listingblock + div.ulist > ul > li > p:first-child > code:first-child": [
            {
                "requiretext": "^VK_[A-Z_]+",
                "type": "Constant"
            },
            {
                "requiretext": "^[a-z][a-zA-Z_]+",
                "type": "Parameter"
            }
        ],
        "div.listingblock + div.paragraph + div.ulist > ul > li > p:first-child > code:first-child": {
            "requiretext": "^VK_[A-Z_]+",
            "type": "Constant"
        },
        "div#fundamentals-errorcodes > ul > li > p:first-child > code:first-child": {
            "requiretext": "^VK_[A-Z_]+",
            "type": "Constant"
        },
        "div.openblock + div.ulist > ul > li > p:first-child > code:first-child": {
            "requiretext": "^VK_[A-Z_]+",
            "type": "Constant"
        },
        "h2": "Section",
        "h3": "Section"
    },
    "ignore": [
      ""
    ]
}
